check process elasticsearch
  with pidfile /var/vcap/sys/run/elasticsearch/elasticsearch.pid
  start program "/var/vcap/jobs/elasticsearch/bin/monit_debugger elasticsearch_ctl '/var/vcap/jobs/elasticsearch/bin/elasticsearch_ctl start'" as uid "vcap" and gid "vcap"
  stop program "/var/vcap/jobs/elasticsearch/bin/monit_debugger elasticsearch_ctl '/var/vcap/jobs/elasticsearch/bin/elasticsearch_ctl stop'" as uid "vcap" and gid "vcap"
  group vcap
